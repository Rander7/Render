		if ((y % 4 == 0) && (y % 100 != 0) || (y % 400 == 0))
		{
			printf("%d ", y);
		}
	}
	return 0;
}

#include<stdio.h>
int get_large(int x, int y)
{
	if (x > y)
		return x;
	else
		return y;
}
int main()
{
	int a = 0, b = 0, max = 0;
	printf("input a,b:\n");
	scanf("%d%d", &a, &b);
	max = get_large(a, b);
		printf("%d\n", max);
	return 0;
}
#include<stdio.h>
#include<math.h>
int is_prime(int x)
{
	int y = 0;
	for (y = 2; y < sqrt(x); y++)
	{
		if (x % y == 0)
		{
			return 0;
		}
	}
	return 1;//这个放在for外面
}
int main()
{
	int n = 0;
	printf("input n:\n");
	scanf("%d", &n);
	if (is_prime(n) == 1)
		printf("yes");
	else
		printf("no");
	return 0;
}
#include<stdio.h>
#include<math.h>
int is_prime(int x)
{
	
	for (int j = 2; j <= sqrt(x); j++)
	{
		
		if (x % j == 0)
		{
			return 0;
		}
	}
	return 1;
}
int main()
{
	int i = 0;
	for (i = 3; i <= 100; i++)
	{
		if (is_prime(i) == 1)
		{
			printf("%d  ", i);
		}
	}
	return 0;
}
#include<stdio.h>
int is_leap_year(int x)
{
	return((x % 4 == 0) && (x % 100 != 0) || (x % 400 == 0));
}
int main()
{
	int y = 0;
	for (y = 1000; y <= 2000; y++)
	{
		if (is_leap_year(y) == 1)
			printf("%d ", y);

	}
	return 0;
}
#include<stdio.h>
int is_leap_year(int x)
{
	return((x % 4 == 0) && (x % 100 != 0) || (x % 400 == 0));
}

int main()
{
	int year = 0;
	printf("请输入年份:");
	scanf("%d", &year);
	if (is_leap_year(year) == 1)
		printf("this year is a leap year");
	else
		printf("this year is not a leap year");

	return 0;
}

#include<stdio.h>
int main()
{
	int i = 0;
	int sum = 1, n;
	scanf("%d", &n);
	for (i = 1; i <= n; i++)//i<=n
	{
		sum = sum * i;
		
	}
	printf("%d\n", sum);
	return 0;
}
#include<stdio.h>
int main()
{
	int n=0, i = 0, j=0,s=0;
	printf("请输入n的值\n");
	scanf("%d", n);
	for (i = 1; i <= n; i++)
	{
		int sum = 1;
		for (j = 1; j <= i; j++)
		{
			sum *= j;
			
		}
		s += sum;
	}
	printf("%d\n", s);
	return 0;
}
#include<stdio.h>
int f(int n)
{
	int fac;
	if ((n == 1)||(n==0)) 
		fac = 1;
	else
		fac = f(n - 1) * n;
	return fac;
}
int main()
{
	int sum = 0;
	int n = 0;
	printf("请输入n的值");
	scanf("%d\n", &n);
	sum = f(n);
	printf("%d!=%d", n, sum);
	return 0;
}
#include<stdio.h>
int f(int n)
{
	int fac=0;
	if ((n == 1) || (n == 0))
		fac = 1;
	else
		fac= f(n - 1) * n;
	return fac;
}
int main()
{
	int n = 0;
	int sum = 0;
	printf("请输入n的值");
	scanf("%d", &n);
	sum = f(n);
	printf("%d!=%d", n, sum);
	return 0;
}

#include<stdio.h>
#include<string.h>
void move(char x, char y)
{
	printf("%c------>%c", x, y);
}
void hannuo(int n, char one, char two, char three)
{
	if (n == 1)
		move(one, three);
	else
	{
		hannuo(n - 1, one, three, two);
		move(one, three);
		hannuo(n - 1, two, one, three);
	}
}
int main()
{
	int n = 0;
	printf("Please input the number of hannuo");
	scanf("%d", n);
	hannuo(n, 'A', 'B', 'C');
	

	
	
	return 0;
}
#include<stdio.h>
void main()
{
	float pi, h, r, c, s, sq, vq, vz;
	pi = 3.1415926;
	printf("input r,h:\n");
	scanf("%f%f", &r, &h);
	c = 2 * pi * r;
	s = r * r * pi;
	sq = 4 * pi * r * r;
	vq = 4.0 / 3.0 * pi * r * r * r;
	vz = pi * r * r * h;
	printf("c=%6.2f\n", c);
	printf("s=%6.2f\n", s);
	printf("sq=%6.2f\n", sq);
	printf("vq=%6.2f\n", vq);
	printf("vz=%6.2f\n", vz);

}
