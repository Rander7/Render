#include<stdio.h>
int my_strlen(char* str)
{
	if (*str != '\0')
		return 1 + my_strlen(str + 1);
	else
	return 0;
}
int main()
{
	char arr[] = "rcw";
	printf("%d\n", my_strlen(arr));
return 0;
}
#include<stdio.h>
int my_strlen(char* str)
{
	if (*str != 0)
		return 1 + my_strlen(str + 1);
	else
		return 0;
}
int main()
{
	char arr[] = "0";
	printf("input arr:\n");
	scanf("%s", arr);
	printf("%d\n", my_strlen(arr));
	return 0;
}
