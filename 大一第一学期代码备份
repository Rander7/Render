#include<stdio.h>
int main()
{
	int i, j, n, s1;
	scanf("%d", &n);
	s1 = 0;

	for (i = 1; i <= n; i++)
	{
		for (j = 1; j <= i; j++)
		{
			s1 += j;
		}


	}
	//#include<stdio.h>
int main()
{
	int i, j, n, s1;
	scanf("%d", &n);
	s1 = 0;

	for (i = 1; i <= n; i++)
	{
		for (j = 1; j <= i; j++)
		{
			s1 += j;
		}


	}
	
	printf("%d", s1);
	return 0;
}


#include<stdio.h>
int main()
{
	float z, m, k, s, i;//这里z，m都要用float
	float sum = 0;
	m = 1;
	z = 2;
	for (i = 1; i <= 20; i++)
	{
		s = z / m;
		sum += s;
		k = z;
		z = m + z;
		m = k;


	}
	printf("%f", sum);
	return 0;
}

#include<stdio.h>
int main()
{
	int a = 0, b = 0, c = 0;
		float sum = 0;
	
	for (a = 0; a < 9; a++)
	{
		for (b = 0; b < 12; b++)
		{
			c = 36 - a - b;
			sum = 4 * a + 3 * b + c / 2.0;
			if (sum == 36)
				printf("%d\n%d\n%d\n", a, b, c);
		}
	}
	return 0;
}
#include<stdio.h>
int main()
{
	int s = 7;
	while (!(s%3==2&&s%5==4&&s%6==5))
	{
		s += 14;
	}
		printf("%d", s);

	return 0;

}


#include<stdio.h>
int main()
{
	float h, w, BMI;
	
	scanf("%f%f", &h, &w);
	BMI = w / (h * h);
	if (BMI < 18.5)
		printf("过轻");
	if (BMI >= 18.5 && BMI < 23.9)
		printf("正常");
	if (BMI >= 24 && BMI < 27.9)
		printf("过重");
	if (BMI >= 28 && BMI < 32)
		printf("肥胖");
	if (BMI >= 32)
		printf("过于肥胖");
					printf("%f", BMI);
					
	return 0;
}

#include<stdio.h>
int main()
{
	int sum = 0, i = 0;
	for (i = 1; sum < 300; i++)
	{
		sum += i;
	}
	i = i - 1;//注意减一
	printf("%d\n%d\n", i, sum);
	return 0;
}

#include<stdio.h>
int main()
{
	int i, j, n, sum = 0;

	scanf("%d", &n);
	for (i = 1; i <= n; i++)
	{
		for (j = 0; j <= i; j++)
		{
			sum += j;
		}
	}
	printf("%d", sum);
	return 0;





}

#include<stdio.h>
int main()
{
	int n;
	float fz=2, fm=1, k=0, fs, sum = 0;
	for (n = 1; n <= 20; n++)
	{
		fs = fz / fm;//这里一定要先写出分数等于什么，再写分子分母怎么变化
		sum += fs;
		k = fz;
		fz = fz + fm;
		fm = k;
	}
	printf("%f", sum);
	return 0;
}
#include<stdio.h>
int main()
{
	int man, woman, child;
	float sum;//记住sum要用float，看170行

	for (man = 0; man < 9; man++)//双循环多个数据都要累加就for套for
	{
		for (woman = 0; woman < 12; woman++)
		{
			child = 36 - man - woman;
			sum = 4 * man + 3 * woman + child / 2.0;
			if (sum==36)
				printf("%d\n%d\n%d\n", man, woman, child);
		}
	}
	return 0;
}

#include<stdio.h>
int main()
{
	int s=7;
	int k = 0;
	while (!(k))
	{
		if (s % 2 == 1 && s % 3 == 2 && s % 5 == 4 && s % 6 == 5 && s % 7 == 0)
		{
			k = 1;
			printf("%d", s);
		}
		s++;
	}
	return 0;

}


#include<stdio.h>
int main()
{
	int year = 0;
	printf("input a year\n");
	scanf("%d",& year);
	if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0)
		printf("it is a leap year\n");
	else
		printf("it is not a leap year\n");
	return 0;
}

#include<stdio.h>
int main()
{
	int grade;
	int k;
	scanf("%d", &grade);
	k = grade / 10;
	switch (k)
	{
	case 10://注意case和数字之间有空格
		case 9:
		printf("A\n");
		break;
		case 8:
		printf("B");
		break;
		case 7:
		printf("C");
			break;
		case 6:
		printf("D");
		break;
	default:
		printf("E");
		break;
	}
	return 0;
}

#include<stdio.h>
int main()
{
	int x, y;
	scanf("%d", &x);
	if (x >= 0)
		y = x * x - 1;
	else
		y = 2*x * x * x - x + 1;
	printf("%d", y);
	return 0;
}
#include<stdio.h>
int main()
{
	int i = 0, sum = 1;
	for (i = 1; i <= 5; i++)
	{
		sum *= i;
	}
	printf("%d\n", sum);
	return 0;
}
#include<stdio.h>
int main()
{
	int i = 0, j = 0, sum = 0, s = 1;
	for (i = 1; i <= 5; i++)
	{
		s = 1;//保证每次s从1开始乘
		for (j = 1; j <= i; j++)
		{
			s *= j;
		}
		sum += s;
	}
	printf("%d\n", sum);
	return 0;
}
#include<stdio.h>
int main()
{
	int i = 0, sum = 0, s = 1;
	for (i = 1; i <= 5; i++)
	{
		s *= i;
		sum += s;
	}
	printf("%d\n", sum);
	return 0;
}
c++的一小步，我的一大步
#include<iostream>
using namespace std;
int main()
{
	cout << "hello world" << endl;
	return 0;
}
#include<stdio.h>
int main()
{
	int i, a[10], max, min, avg, s = 0;
	for (i = 0; i <= 10; i++)
	scanf("%d\n", a[i]);
	max = min = a[0];
	for (i = 0; i <= 10; i++)
	{
		if (a[i] > max)
		
			max = a[i];
		
			if (a[i] < min)
			
				min = a[i];
			
		s += a[i];

	}
	s = s - max - min;
	avg = s / 8;
	printf("%d\n", avg);
	return 0;
}
#include<iostream>
using namespace std;
int main()
{
	int i;
	double  a[10], max, min, avg, s = 0;
	
	cout << "请输入十个分数" << endl;
	for(i=1; i<10; i++)
	cin >> a[i];
	max = min = a[0];
	for (i = 1; i < 10; i++)
	{
		if (a[i] > max) max = a[i];
		if (a[i] <min) min = a[i];
		s += a[i];
	}
	s = s - max - min;
	avg = s / 8;
	cout << "平均值为:" << avg << endl;
	return 0;
}

#include<iostream>
using namespace std;
int main()
{
	int i;
	double a[10], max, min, sum, s = 0, avg;
	cout << "请输入十个数" << endl;
	for (i = 1; i < 10; i++)
		cin >> a[i];
	max = min = a[0];
	for (i = 1; i < 10; i++)
	{
		if (max < a[i]) max = a[i];
		if (min > a[i]) min = a[i];
		s += a[i];
	}
	s = s - max - min;
	avg = s / 8;
	cout << avg << endl;
	return 0;
}


#include<stdio.h>
#include<windows.h>
#include<string.h>
int main()
{
	char arr1[] = "you asshole!!!!!!";
	char arr2[] = "*****************";
	int left = 0;
	int right = strlen(arr1) - 1;
	
	while (left <= right)
	{
		arr2[left] = arr1[left];
		arr2[right] = arr1[right];
		printf("%s\n", arr2);
		Sleep(1000);
		system("cls\n");
		
		left++;
		right--;
	}
	printf("%s\n", arr1);
	return 0;


}


#include<stdio.h>
#include<string.h>
int main()
{
	char password[20] = {0};
	
	int i = 0;
	for (i = 0; i <3; i++)
	{
		scanf("%s", password);
		if (strcmp(password, "123456") == 0)
		{
			printf("right\n");
			break;
		}
		else
		{
			printf("wrong\n");
			
		}
	}
	if (i == 3)
		printf("error\n");
	return 0;
}
#include<stdio.h>
#include<string.h>
int main()
{
	char password[20] = { 0 };
	int i = 0;
	for (i = 0; i < 3; i++)
	{
		printf("请输入密码：\n");
		scanf("%s", password);
		if (strcmp(password, "123456") == 0)
		{
			printf("correct\n");
			break;
		}
		else
		{
			printf("wrong\n");
		}
	}
	if (i == 3)
		printf("error\n");
	return 0;
}
#include<stdio.h>
#include<string.h>
int main()
{
	int i = 0;
	char password[20] = { 0 };
	for (i = 0; i < 3; i++)
	{
		printf("please input your password\n");
		scanf("%s", password);//注意这里scanf的password不加&,这个放for里面
		if (strcmp(password, "123456") == 0)
		{
			printf("password is correct\n");
			break;
		}
		else
		{
			printf("please input again\n");
		}
	}
	if (i == 3)
		printf("wrong password");
	return 0;


}



#include<stdio.h>
int main()
{
	int i, n, s = 1, sum = 0;
	printf("请输入n\n");
	scanf("%d", &n);
	for (i = 1; i <= n; i++)
	{
		s *= i;
		sum += s;
	}
	printf("%d\n", sum);
	return 0;


}
#include<stdio.h>
int main()
{
	int i, n, j, s = 1, sum = 0;
	scanf("%d", &n);
	for (i = 1; i <= n; i++)
	{
		s = 1;
		for (j = 1; j <= i; j++)
		{
			s *= j;
		}
		sum += s;
	}
	printf("%d\n", sum);
	return 0;
}

