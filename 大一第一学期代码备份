	printf("%d\n", my_strlen(arr));
	return 0;
}
#include<stdio.h>
void print_table(int n)
{
	int i = 0, j = 0;
	for (i = 1; i <= n; i++)
	{
		for (j = 1; j <= i; j++)
		{
			printf("%d*%d=%d ", i, j, i * j);
		}
		printf("\n");
	}
}
int main()
{
	int n = 0;
	printf("input n:\n");
	scanf("%d", &n);
	print_table(n);
	return 0;
}
#include<stdio.h>
int main()
{
	int arr[] = {1,2,3,4,5,6,7,8,9};
	
	int i = 0;
	int sz = sizeof(arr) / sizeof(arr[0]);
	int* p = arr;
	for (i = 0; i < sz; i++)
	{
		printf("%d ",*p);
		p++;
	}
	
	return 0;
}
