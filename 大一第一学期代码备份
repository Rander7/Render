int Add()
{
	int i = 0;
	int count = 0;
	for (i = 100; i<=999; i++)
	{
		if ((i / 100 != i % 100 / 10) && (i / 100 != i % 100 % 10) && (i % 100 / 10 != i % 100 % 10) && (i % 2 == 0))
			count++;
	}
	return count;

}
