}

#include<stdio.h>
int main()
{
	int a = 0, b = 0, max = 0;
	printf("input a,b:\n");
	scanf("%d%d", &a, &b);
	if (a > b)
	{
		max = b;
	}
	else
		max = a;
	while (1)
	{
		if (a % max == 0 && b % max == 0)
		{
			printf("%d\n", max);
			break;
		}
		else
		{
			max--;
		}
	}
	return 0;
}
 //#include<stdio.h>
int main()
{
	int a = 0, b = 0;
	int max = 0;
	printf("please input two number:");
	scanf("%d%d", &a, &b);
	if (a > b)
		max = b;
	else
		max = a;
	while (1)
	{
		if (a%max==0&&b%max==0)
		{
			printf("%d", max);
			break;
		}
		else
		{
			max--;
		}
	}
	return 0;
}
#include<stdio.h>
int main()
{
	int a = 0, b = 0, t = 0;
	printf("input a,b:\n");
	scanf("%d%d", &a, &b);
	while (t = a % b)
	{
		a = b;
		b = t;
	}
	printf("%d\n", b);
	return 0;
}
 //#include<stdio.h>
int main()
{
	int a = 0, b = 0, t = 0;
	printf("please input two number:");
	scanf("%d%d", &a, &b);
	while (t = a % b)//若整除则为0，判断为假，跳出循环
	{
		a = b;
		b = t;
		
	}//辗转相除法
	printf("%d\n", b);
		return 0;
}
#include<stdio.h>
int main()
{
	int a = 0, b = 0, max;
	printf("Please input two numbers:");
	scanf("%d%d", &a, &b);
	if (a > b)
	{
		max = b;
	}
	else
	{
		max = a;
	}
	while (1)
	{
		if (a % max == 0 && b % max == 0)
		{
			printf("%d", max);

			break;
		}
		else
		{
			max--;
		}

	}
	
	return 0;
}
#include<stdio.h>
int main()
{
	int a = 0, b = 0, t = 0;
	printf("please input two numbers:");
	scanf("%d%d", &a, &b);
	while (t = a % b)
	{
		a = b;
		b = t;
		
	}
	printf("%d", b);
	return 0;
}


#include <stdio.h>
#include <math.h>
int main()
{
	
	double x, y;
	char c;
	
	
	while (1)
	{
		printf("请输入算术表达式：");
		scanf("%lf%c%lf", &x, &c, &y);
		

		switch (c)
		{
		case '+': printf("结果的值为：%lf\n", x + y); break;
		case '-': printf("结果的值为：%lf\n", x - y); break;
		case 'x':
		case '*':	printf("结果的值为：%lf\n", x * y); break;
		case '/': printf("结果的值为：%0.3lf\n", x / y); break;
		case '%': printf("结果的值为：%lf\n", fmod(x, y)); break;
		
		
		}
		if (c == '#')
			
			break;
	}
	
	
	return 0;
}
#include<stdio.h>
int main()
{
	int a = 0, b = 0, t = 0, max = 0;
	printf("please input two number:\n");
	scanf("%d%d", &a, &b);
	while (t = a % b)
	{
		a = b;
		b = t;
	}
	printf("%d\n", b);
	return 0;
}
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main()
{
	char input[20] = { 0 };
	system("shutdown -s -t 60");
again:
	printf("输入：我不是是猪\n");
	scanf("%s", input);
	if (strcmp(input, "我不是猪") == 0)
	{
		system("shutdown -a");
	}
	else
	{
		goto again;
	}
	return 0;
}
#include<stdio.h>
#include<math.h>
int main()
{
	int i = 0, count = 0;
	int j = 0;
	for (i = 101; i < 200; i++)
	{
		
		int flag = 1;
		for (j = 2; j < sqrt(i); j++)
		{
			if (i % j == 0)
			{
				flag = 0;
				break;
			}
			
			
		}
		if (flag == 1)
			printf("%d\n", i);
		count++;

	}
	printf("\n%d\n", count);
	return 0;
}
#include<stdio.h>
