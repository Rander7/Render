#include<stdio.h>
int Add(int n)
{
	int i = 0;
	int sum = 0;
	for (i = 1; i <= n; i++)
	{
		sum += i;
	}
	return sum;
}
int main()
{
	int n = 0;
	scanf("%d", &n);
	Add(n);
	printf("%d\n", Add(n));
	return 0;
}
#include<stdio.h>
int Is_prime(int n)
{
	int i = 0;
	int j = 0;
	if (n == 1)
	{
		return 0;
	}
	else
	{
		for (i = 2; i < n; i++)
		{
			j = n % i;
			if (j == 0)
			{
				return 0;
			}
			else
				return 1;
		}
	}
}
int main()
{
	int n = 0;
	int s = 0;
	scanf("%d",&n);
	s = Is_prime(n);
	if (s == 0)
	{
		printf("该数不是素数\n");
	}
	else
		printf("该数是素数\n");
	return 0;
}
#include<stdio.h>
int Gy(int m, int n)
{
	int t = 0;
	do {
		t = m % n;
		m = n;
		n = t;
	} while (t);
		return m;
}
int main()
{
	int m = 0;
	int n = 0;
	scanf("%d%d", &m, &n);
	printf("%d\n", Gy(m, n));
	
	
	return 0;
}
#include<stdio.h>
int Gy(int m, int n)
{
	if (m <= 0 || n <= 0)
		return 0;
	if (m % n == 0)
		return n;
	else Gy(n, m % n);
}
int main()
{
	int m = 0;
	int n = 0;
	scanf("%d%d", &m, &n);
	printf("%d\n", Gy(m, n));


	return 0;
}

#include<stdio.h>
double Add(int n)
{
	if (n == 1)
		return 1;
	if (n > 1)
	{
		double sum = 0;
		int i = 0;
		double s = 0;
		for (i = 1; i <= n; ++i)
		{
			s = 1.0 / (i * (i + 1));
			sum += s;

		}
		sum += 1;
		return sum;
	}
}
int main()
{
	int n = 0;
	scanf("%d", &n);
	printf("%lf\n", Add(n));
	return 0;
}
#include<stdio.h>
double Add(int n)
{
	int i = 1;
	double f = -1.0;
	double s = 0;
	double sum = 0;
	
	
		for (i = 0; i <= n; i++)
		{
			f = (-1) * f;
			s = f / (2 * i + 1);
			sum += s;
		}
		return sum;

}
int main()
{

	int n = 0;
	scanf("%d", &n);
	printf("%lf\n", Add(n-1));
	return 0;
}
#include<stdio.h>
int Add(int n)
{
	int i = 0;
	int s = 0;
	int t = 1;
	for (i = 1; i <= n; i++)
	{
		t *= i;
		s += t;
	}
	return s;
}
int main()

{
	int n = 0;
	scanf("%d", &n);
		printf("%d\n", Add(n));
	return 0;
}
