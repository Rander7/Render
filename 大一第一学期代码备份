	for (i = 1; i <= n; i++)
	{
		s *= i;
		sum += s;
	}
	printf("%d\n", sum);
	return 0;
}
#include<stdio.h>
int main()
{
	int i = 0, j = 0, sum = 0, s = 1, n = 0;
	printf("input n\n");
	scanf("%d", &n);
	for (i = 1; i <= n; i++)
	{
		s = 1;
		for (j = 1; j <=i; j++)
		{
			s *= j;
		}
		sum += s;
	}
	printf("%d\n", sum);
	return 0;
}
