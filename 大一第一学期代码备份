#include<stdio.h>
void Add(int a[], int n)
{
	int i = 0;
	int j = 0;
	int t = 0;
	for (i = 0; i < n-1; i++)
	{
		for (j = 0; j < n - 1; j++)
		{
			if (a[j] > a[j + 1])
			{
				t = a[j];
				a[j] = a[j + 1];
				a[j + 1] = t;
			}
		}
	}
	
}
int main()
{
	int i = 0;
	int n = 0;
	scanf("%d", &n);
	int a[256] = { 0 };
	for (i = 0; i < n; i++)
	
		scanf("%d", &a[i]);
	
	Add(a, n);
	for (i = 0; i < n; i++)
	{
		printf("%d", a[i]);
	}
	return 0;
}
#include<stdio.h>
void bubble(int a[], int n)
{
	int i, j, t;
	for (i = 0; i < n - 1; i++)
	{
		for (j = 0; j < n - 1; j++)
		{
			if (a[j] > a[j + 1])
			{
				t = a[j];
				a[j] = a[j + 1];
				a[j + 1] = t;
			}
		}
	}
}
int main()
{
	int i = 0;
	int a[] = { 2,87,4,5,2,67 };
	int n = sizeof(a) / sizeof(a[0]);
	bubble(a, n);
	for (i = 0; i < n; i++)
	{
		printf("%d ", a[i]);
	}
	return 0;
}
#include<stdio.h>
int Add(int a[],int n)
{
	int i = 0;
	int count = 0;
	for (i = 0; i < n; i++)
	{
		if (a[i] % 2 != 0)
			count++;
	}
	return count;
}
int main()
{
	int a[] = { 3,56,67,2,4,56,7,78 };
	int n = sizeof(a) / sizeof(a[0]);
	printf("%d\n",Add(a,n));
	return 0;
}
#include<stdio.h>
int fun(int num)
{
	int a = 0, p = 1,s=0, sum = 0;
	while (1)
	{
		a = num % 2;
		s = p * a;
		sum += s;
		p *= 10;
		num /= 2;
		if (num < 2)
		{
			sum += p * num;
			break;
		}
	}
	return sum;
}
int main()
{
	int num = 0;
	scanf("%d", &num);
	printf("%d\n", fun(num));
	return 0;

}
 //#include<stdio.h>
int fun(int num)
{
	int p = 1, sum = 0, a;
	while(1)
	{
		a = num % 2;
		num /= 2;
		sum += a * p;
		p *= 10;
		if (num < 2)
		{
			sum += num * p;
			break;
		}
	}
	return sum;

}
int main()
{
	int num;
	scanf("%d", &num);
	printf("%d\n", fun(num));
	return 0;
}
#include<stdio.h>
int fun(int num)
{
	int a = 0, b = 0, c = 0;
	int d = 1;
	while (1)
	{
		a = num % 16;
		b = d * a;
		c += b;
		num /= 16;
		d *= 10;
		if (num < 16)
		{
			c += num * d;
			break;
		}
		
	}
	return c;
}
int main()
{
	int num;
	scanf("%d", &num);
	printf("%d\n", fun(num));
	return 0;
}
#include<stdio.h>
#include<math.h>
void Add(int* a, int k)
{
	int i = 0;
	int j = 0;
	int t = 0;
	for (i = 0; i < k-1; i++)
	{
		for (j = 0; j < k-1; j++)
		{
			if (fabs((double)a[j]) > fabs((double)a[j + 1]))
			{
				t = a[j];
					a[j] = a[j + 1];
					a[j + 1] = t;
			}
		}
			
	}
	
}
int main()
{
	int a[256] = { 0 };
	int k = 0;
	int i = 0;
	while (1)
	{
		scanf("%d", &a[k]);
		k++;
		if (getchar() == '\n')
			break;
	}
	Add(a, k+1);
	for (i = 1; i <= k; i++)
	{
		printf("%d ", a[i]);
	}
	return 0;
}
#include<stdio.h>
void change(int* a, int* b)
{
	int t = 0;
	t = *a;
	*a = *b;
	*b = t;
	
}
int main()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);
	change(&a, &b);
	printf("%d %d",a, b);
	return 0;
}
#include<stdio.h>
void find(int* a, int k)
{
	int i = 0;
	int j = 0;
	int t = 0;
	for (i = 0; i < k; i+=2)
	{
		for (j = 0; j < k ; j += 2)
		{
			if (a[j] > a[j + 2])
			{
				t = a[j];
				a[j] = a[j + 2];
				a[j + 2] = t;
			}
		}
	}
}

int main()
{
	int a[256] = { 0 };
	int k = 0;
	int i = 0;
	while (1)
	{
		scanf("%d", &a[k]);
		k++;
		if (getchar() == '\n')
			break;
	}
	find(a, k + 1);
	
		for (i = 1; i <= k / 2; i += 2)
			printf("%d ", a[i]);
	

}
#include<stdio.h>
int fun(int a, int b)
{
	
	if (a % b == 0)
	{
		return b;
	}
	else
	{
		 fun(b, a % b);
	}
}
int main()
{
	int a = 0;
	int b = 0;
	scanf("%d%d", &a, &b);
	printf("%d\n", fun(a, b));
	return 0;
}
#include<stdio.h>
void fun(int a[], int n)
{
	int i, j,t, tmp;
	for (i = 1; i < n - 1; i ++)
	{
	
		for (j = i + 1 ; j<= n - 1; j ++)
		{
			if (a[i] > a[j])
			{
				
				tmp = a[i];
				a[i] = a[j];
				a[j] = tmp;
			}
		}
	}
}
	
int main()
{
	int a[] = { 2,56,23,78,12,6 };
	fun(a, 6);
	int i = 0;
	for (i = 0; i < 6; i++)
		printf("%d ", a[i]);
	return 0;
}
#include<stdio.h>
#include<string.h>
void fun(char a[])
{
	char* p = a, * q = a, c;
	int i;
	while (*p != '\0')
		p++;
	p--;
	while (p > q)
	{
		c = *p;
		*p = *q;
		*q = c;
		p--;
		q++;
	}
}
int main()
{
	char a[100];
	scanf("%s", a);
	fun(a);
	printf("%s", a);
	return 0;
}
#include<stdio.h>
#include<string.h>
void fun(char a[], char b[])
{
	int n, k;
	n = strlen(a);
	k = strlen(b);
	int i = 0;
	for (i = 0; i < k; i++)
	{
		a[n + i] = b[i];
	}
}
int main()
{
	char a[100];
	char b[50];
	
	gets_s(a);
	gets_s(b);
	
	fun(a, b);
	puts(a);
	return 0;
}
#include<stdio.h>
#include<string.h>
void fun(char a[], char b[])
{
	int n;
	int k;
	n = strlen(b);
	k = strlen(a);
	int i = 0;
	for (i = 0; i < n; i++)
	{
		a[k + i] = b[n - 1 - i];
	}
}
int main()
{
	char a[10];
	char b[10];

	gets_s(a);
	gets_s(b);

	fun(a, b);
	puts(a);
	return 0;
}
#include<stdio.h>
int find(int a[], int n)
{
	int i = 0;
	for (i = 0; i < n; i++)
	{
		if (a[i] == n)
		{
			return 1;
		}

	}
	return 0;
}
int main()
{
	int a[5];
	int i = 0;
	int n = 0;
	for (i = 0; i < 5; i++)
		scanf("%d", &a[i]);
	printf("输入n\n");
	scanf("%d", &n);
	printf("%d\n", find(a, n));
	return 0;
}
#include<stdio.h>

int letter = 0, number = 0, space = 0, other = 0;
void fun(char a[])
{
	int i;
	
	for (i = 0; a[i] != '\0'; i++)
	{
		if ((a[i] >= 'a' && a[i] <= 'z') || (a[i] >= 'A' && a[i] <= 'Z'))
		{
			letter++;
		}
		else if (a[i] <= '9' && a[i] >= '0')
		{
			number++;
		}
		else if (a[i] == ' ')
		{
			space++;
		}
		else
		{
			other++;
		}

	}
}
int main()
{
	
	char a[100];
	gets_s(a);
	fun(a);
	printf("letter=%d\n", letter);
	printf("number=%d\n", number);
	printf("space=%d\n", space);
	printf("other=%d\n", other);
	return 0;
}
#include<stdio.h>
double Add(int n)
{
	int i = 0;
	double s = 1;
	
		for (i = 1; i<=n; i++)
		{
			s += 1.0 / (i * (i + 1));

		}
		
		return s;
	

}
int main()
{
	int n = 0;
	scanf("%d", &n);
	printf("%lf\n", Add(n));
	return 0;
}
