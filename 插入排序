#include<stdio.h>//插入排序
int main()
{
	int a[5];
	int i = 0;
	for (i = 0; i < 5; i++) {
		scanf("%d", &a[i]);
	}
	int j, t;
	for (i = 1; i < 5; i++) {//先确定第二个元素为基准，此后不断比较往前插入元素
		t = a[i];//令t储存这个基准数
		for (j = i - 1; j >= 0 && a[j] > t; j--) {//如果前面一个数比这个数大
			a[j + 1] = a[j];//让基准数赋值成前一个较大的数
		}//j--后循环 98765-->99765-->89765-->89965-->88965-->78965   
		a[j + 1] = t;//跳出for循环时候t会赋值给前面比他小之后的位置的数    
	}//能进内循环说明基准数小，具体t赋给谁在于t和之前已确定的数的比较
	for (i = 0; i < 5; i++) {//89965则先变为88965，第一个赋值为7，即78965
		printf("%d ", a[i]);//如果是59965，则第二次内循环不进行直接第二个数赋值为t，即57965
	}
	return 0;
}
